{
  "name": "meetify",
  "version": "1.0.0",
  "description": "Application for meeting new people based on your music interests (via Spotify)",
  "author": "Dustin Seger",
  "private": true,
  "main": "./public/electron.js",
  "homepage": "./",
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@reduxjs/toolkit": "^1.5.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "axios": "^0.21.1",
    "electron-is-dev": "^1.2.0",
    "jquery": "^3.5.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.2",
    "react-scripts": "^4.0.1",
    "react-transition-group": "^4.4.1",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "website:serve": "react-scripts start",
    "website:build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron:serve": "concurrently \"npm run website:serve\" \"wait-on http://localhost:3000 && electron .\"",
    "electron:build": "npm run website:build && electron-builder"
  },
  "postInstall": "electron-builder install-app-deps",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "electron": "^11.1.1",
    "electron-builder": "^22.9.1",
    "typescript": "^4.1.3",
    "wait-on": "^5.2.1"
  },
  "build": {
    "appId": "meetify.ui",
    "productName": "Meetify",
    "extends": null,
    "files": [
      "build/**",
      "public/**"
    ]
  }
}
